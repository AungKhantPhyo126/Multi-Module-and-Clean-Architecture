<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/dailyGoldPriceFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.critx.shwemiAdmin.screens.auth.LoginFragment"
        tools:layout="@layout/fragment_login"/>
    <fragment
        android:id="@+id/dailyGoldPriceFragment"
        android:name="com.critx.shwemiAdmin.screens.dailyGoldPrice.DailyGoldPriceFragment"
        tools:layout="@layout/fragment_daily_gold_price">
        <argument
            android:name="token"
            app:argType="string" />
        <action
            android:id="@+id/action_dailyGoldPriceFragment_to_priceByTableFragment"
            app:destination="@id/priceByTableFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
        <action
            android:id="@+id/action_dailyGoldPriceFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/priceByTableFragment"
        android:name="com.critx.shwemiAdmin.screens.dailyGoldPrice.priceByTable.PriceByTableFragment"
        tools:layout="@layout/fragment_price_by_table" />
    <fragment
        android:id="@+id/sampleTakeAndReturnFragment"
        android:name="com.critx.shwemiAdmin.screens.sampleTakeAndReturn.SampleTakeAndReturn"
        android:label="SampleTakeAndReturn" />
    <fragment
        android:id="@+id/confirmVoucherFragment"
        android:name="com.critx.shwemiAdmin.screens.confirmVoucher.ConfirmVoucherFragment"
        android:label="ConfirmVoucherFragment" >
        <action
            android:id="@+id/action_confirmVoucherFragment_to_unConfirmVoucherFragment"
            app:destination="@id/unConfirmVoucherFragment" />
    </fragment>
    <fragment
        android:id="@+id/unConfirmVoucherFragment"
        android:name="com.critx.shwemiAdmin.screens.confirmVoucher.unconfirmVoucher.UnConfirmVoucherFragment"
        android:label="UnConfirmVoucherFragment" />
    <fragment
        android:id="@+id/pointFragment"
        android:name="com.critx.shwemiAdmin.screens.point.PointFragment"
        android:label="PointFragment" />
    <fragment
        android:id="@+id/discountFragment"
        android:name="com.critx.shwemiAdmin.screens.discount.DiscountFragment"
        android:label="DiscountFragment" />
    <fragment
        android:id="@+id/flashSaleFragment"
        android:name="com.critx.shwemiAdmin.screens.flashsale.FlashSaleFragment"
        android:label="FlashSaleFragment" />
    <fragment
        android:id="@+id/collectStockFragment"
        android:name="com.critx.shwemiAdmin.screens.collectstock.CollectStockFragment"
        android:label="CollectStockFragment" >
        <action
            android:id="@+id/action_collectStockFragment_to_fillInfoCollectStockFragment"
            app:destination="@id/fillInfoCollectStockFragment" />
    </fragment>
    <fragment
        android:id="@+id/fillInfoCollectStockFragment"
        android:name="com.critx.shwemiAdmin.screens.collectstock.fillinfo.FillInfoCollectStockFragment"
        android:label="FillInfoCollectStockFragment" />
    <fragment
        android:id="@+id/setupStockFragment"
        android:name="com.critx.shwemiAdmin.screens.setupStock.SetupStockFragment"
        android:label="SetupStockFragment" />
</navigation>